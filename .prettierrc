{
    "$schema": "http://json.schemastore.org/prettierrc",
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 100,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false,
    "plugins": ["prettier-config-moon", "prettier-plugin-tailwindcss"]
}
