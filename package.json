{
    "name": "moonlight",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "deps": "npx --yes npm-check-updates -x typescript --deep --interactive",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
        "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint . --fix",
        "format": "prettier --write \"**/*.{json,js,jsx,ts,tsx}\"",
        "moon": "moon --color --log trace"
    },
    "devDependencies": {
        "@moonrepo/cli": "^1.9.2",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.3",
        "babel-preset-moon": "^2.0.5",
        "eslint-config-moon": "^2.0.5",
        "eslint": "^8.44.0",
        "jest-preset-moon": "^2.0.2",
        "jest": "^29.5.0",
        "prettier-config-moon": "^1.1.2",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "prettier": "^2.8.8",
        "tsconfig-moon": "^1.3.0",
        "typescript": "5.0.4"
    },
    "packageManager": "pnpm@8.6.5",
    "engines": {
        "node": "18.16.1"
    }
}
